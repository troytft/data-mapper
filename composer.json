{
    "name" : "troytft/data-mapper-bundle",
    "description": "Allow mapping data to models",
    "type" : "symfony-bundle",
    "license" : "MIT",
    "require" : {
        "php": ">=8.0",
        "doctrine/doctrine-bundle": "^1.12 || ^2.2",
        "doctrine/orm": "^2.5",
        "symfony/framework-bundle": "^4.4 || ^5.2",
        "symfony/validator": "^4.4 || ^5.2",
        "symfony/yaml": "^4.4 || ^5.2"
    },
    "autoload" : {
        "psr-4" : {
            "Troytft\\DataMapperBundle\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/src",
            "TestApp\\": "tests/test-app"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "nyholm/symfony-bundle-test": "^2.0",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^9.5"
    }
}
